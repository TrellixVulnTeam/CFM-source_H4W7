{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
       "ecmaVersion": 9,
       "sourceType": "module",
       "ecmaFeatures": {
           "jsx": true
       }
   },
  "env": {
    "node": true,
		"browser": true,
		"es6": true
  },
  "rules": {
    "no-console": "off",
    "react/jsx-filename-extension": "off",
    "semi": 2,
    "indent": ["error", "tab"]

  }
}
